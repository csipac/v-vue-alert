{
  "name": "v-vue-alert",
  "version": "1.2.0",
  "description": "Alert bulmca colors for vue.js",
  "main": "dist/vuealert.common.js",
  "scripts": {
    "build": "rollup -c config/rollup.config.js",
    "build:example": "NODE_ENV=production node config/build.js",
    "clean": "rimraf dist/*",
    "codecov": "codecov",
    "dev": "cross-env NODE_ENV=development node config/dev-server.js",
    "lint": "eslint --ext .js,.vue .",
    "test": "npm run lint && jest --coverage",
    "test:dev": "jest --watchAll",
    "prebuild": "npm run clean -s",
    "preversion": "npm run build && npm run test && npm run build:example",
    "version": "git add -A dist && git add -A example/dist",
    "postversion": "git push origin master --follow-tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csipac/v-vue-alert.git"
  },
  "engines": {
    "node": ">=4"
  },
  "pre-commit": [
    "build",
    "test"
  ],
  "keywords": [
    "bulmacss",
    "alert",
    "vue",
    "vue-alert-bulma",
    "v-vue-alert"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    }
  },
  "author": "devsipac <wilmerchuluc@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/csipac/v-vue-alert/issues"
  },
  "homepage": "https://github.com/csipac/v-vue-alert#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-vue": "^2.0.0",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.2.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "jest-cli": "^22.1.2",
    "jest-vue-preprocessor": "^1.3.1",
    "lodash": "^4.17.4",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.54.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-vue": "^3.0.0",
    "uglify-es": "^3.3.7",
    "vue-loader": "^13.7.0",
    "vue-template-compiler": "2.x",
    "@vue/test-utils": "1.0.0-beta.10",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "vue": "2.x"
  }
}
